{
  "name": "@pbuilder/cli",
  "version": "1.8.0",
  "description": "Project Builder - modern, fast, powerful node.js web framework to create complex projects and execute schematics",
  "private": false,
  "engines": {
    "node": ">= 20"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "builder": "bin/builder.js"
  },
  "scripts": {
    "build": "vite build",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:watch": "concurrently \"vite build --watch\" \"tsc --declaration --emitDeclarationOnly --outDir dist --watch\"",
    "format": "biome format --write",
    "test:local": "bun uninstall @pbuilder/cli -g && bun run build && cd dist && bun link",
    "publish:verdaccio": "bun run build && cd dist && bun publish --registry http://localhost:4873",
    "publish": "bun run build && cd dist && bun publish --access=public",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyperxq/Project-builder-cli"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "Schematics",
    "Angular 17+",
    "Nestjs"
  ],
  "author": "Hyperxq",
  "license": "ISC",
  "dependencies": {
    "@angular-devkit/core": "^19.0.4",
    "@angular-devkit/schematics": "^19.0.4",
    "@angular-devkit/schematics-cli": "^19.0.4",
    "@danils/angular-builder": "^0.0.67",
    "@pbuilder/sm": "1.4.4",
    "@schematics/angular": "^17.0.1",
    "@types/npm-registry-fetch": "^8.0.7",
    "ansi-colors": "^4.1.3",
    "axios": "^1.7.9",
    "case-anything": "^2.1.13",
    "commander": "^11.1.0",
    "inquirer": "8.2.6",
    "node-emoji": "^2.1.0",
    "npm-package-arg": "^11.0.1",
    "npm-registry-fetch": "^16.1.0",
    "ora": "5.4.1",
    "winston": "^3.11.0",
    "winston-console-format": "^1.0.8",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/inquirer": "^9.0.6",
    "@types/node-emoji": "^2.1.0",
    "@types/npm-package-arg": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "concurrently": "^9.1.2",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.5.2",
    "vite": "^6.3.4",
    "vite-plugin-static-copy": "^2.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}