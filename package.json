{
  "name": "@danils/project-builder-cli",
  "version": "0.1.34",
  "description": "Project Builder - modern, fast, powerful node.js web framework to build projects(@cli)",
  "private": false,
  "engines": {
    "node": ">= 16"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "builder": "bin/builder.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "test:local": "npm run build && cd dist && npm link",
    "publish:verdaccio": "npm run build && cd dist && npm publish --registry http://localhost:4873",
    "publish": "npm run build && cd dist && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyperxq/Project-builder-cli"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "Schematics",
    "Angular 16+",
    "Nestjs"
  ],
  "author": "Hyperxq",
  "license": "ISC",
  "dependencies": {
    "@angular-devkit/core": "^17.0.1",
    "@angular-devkit/schematics": "^17.0.1",
    "@angular-devkit/schematics-cli": "^17.0.1",
    "@danils/angular-builder": "^0.0.67",
    "@schematics/angular": "^17.0.1",
    "ansi-colors": "^4.1.3",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "inquirer": "8.2.6",
    "node-emoji": "^2.1.0",
    "ora": "5.4.1",
    "typescript": "^5.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/inquirer": "^9.0.6",
    "@types/node-emoji": "^2.1.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "rimraf": "^5.0.5",
    "rollup": "3.29.4",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  }
}
